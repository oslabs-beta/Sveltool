{
  "name": "sveltool",
  "version": "0.0.1",
  "description": "Dynamic visualization for all things Svelte",
  "private": true,
  "scripts": {
    "watch": "webpack --mode=development --watch --config config/webpack.config.js",
    "dev": "webpack-dev-server --mode=development --config config/webpack.config.js",
    "build": "webpack --mode=production --config config/webpack.config.js",
    "format": "prettier --write --ignore-unknown \"{config,public,src}/**/*.{html,css,js,ts,json}\""
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.19.4",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^8.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^2.7.1",
    "svelte-loader": "^3.1.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "d3": "^7.6.1",
    "d3v4": "^4.2.2",
    "portfinder-sync": "^0.0.2",
    "svelte": "^3.49.0",
    "svelte-icons-pack": "^2.0.0"
  }
}
